<Window x:Class="Adamantium.UI.Window"
        xmlns:x="http://adamantium/ui/xaml/extensions"
        xmlns="clr-namespace:Adamantium.UI;assembly=Adamantium.UI"
        Title="MainWindow" 
        Height="850" 
        Width="800">
    <Grid x:Class="MainWindow" Name="Grid1">
    <Grid.Template>
        <ControlTemplate>
            <Grid>
                <Path StrokeThickness="2" Fill="Red">
                    <Path.Data>
                        <GeometryGroup FillRule="EvenOdd">
                            <RectangleGeometry Rect="25 75 150 50"/>
                            <RectangleGeometry Rect="35 85 120 30"/>
                        </GeometryGroup>
                    </Path.Data>
                </Path>
            </Grid>
        </ControlTemplate>
    </Grid.Template>
    </Grid>
</Window>
